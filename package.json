{
  "name": "insight",
  "displayName": "Insight",
  "description": "Know your app better",
  "publisher": "raathigeshan",
  "version": "0.11.0",
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Raathigesh/insight.git"
  },
  "icon": "icons/icon.png",
  "galleryBanner": {
    "color": "#111111",
    "theme": "dark"
  },
  "main": "./out/extension/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "insight.showPanel",
        "title": "Toggle Insight panel"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run build",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./src/extension --project ./tsconfig.json",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "ui": "webpack-dev-server --env.development --config ./webpack.config.js",
    "build-ui": "webpack --env.production --config ./webpack.config.js",
    "build": "rimraf out && yarn compile && yarn build-ui",
    "package-ext": "vsce package --yarn",
    "publish-ext": "vsce publish --yarn",
    "pretty": "prettier --write  \"src/**/*.{ts,tsx}\"",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@chakra-ui/core": "^0.4.1",
    "@emotion/core": "^10.0.22",
    "@types/babel-traverse": "^6.25.5",
    "@types/jest": "^24.0.21",
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "fuzzysort": "^1.1.4",
    "graphql-tag": "^2.10.1",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "mobx": "^5.14.0",
    "mobx-react-lite": "^1.4.1",
    "mobx-utils": "^5.4.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-feather": "^1.1.6",
    "react-router-dom": "^5.0.0",
    "react-select": "^3.0.8",
    "react-spring": "^8.0.27",
    "react-tippy": "^1.2.3",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "subscriptions-transport-ws": "^0.9.16",
    "typescript": "^3.6.3",
    "vscode": "^1.1.28",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "@babel/traverse": "^7.4.3",
    "@emotion/styled": "^10.0.23",
    "emotion-theming": "^10.0.19",
    "graphql": "^14.3.1",
    "graphql-yoga": "^1.17.4",
    "module-alias": "^2.2.0",
    "nanoid": "^2.0.3",
    "node-fetch": "^2.6.0",
    "react": "^16.8.6",
    "recursive-readdir": "^2.2.2",
    "reflect-metadata": "^0.1.13",
    "tmp": "^0.1.0",
    "type-graphql": "^0.17.4",
    "typedi": "^0.8.0",
    "urql": "^1.0.5",
    "wonka": "^3.2.0",
    "ws": "^7.1.1"
  },
  "resolutions": {
    "graphql": "^14.3.1"
  },
  "_moduleAliases": {
    "common": "./out/common",
    "entities": "./out/entities",
    "indexer": "./out/indexer"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/out/"
    ],
    "moduleNameMapper": {
      "^common(.*)$": "<rootDir>/src/common$1",
      "^entities(.*)$": "<rootDir>/src/entities$1",
      "^indexer(.*)$": "<rootDir>/src/indexer$1"
    }
  }
}
