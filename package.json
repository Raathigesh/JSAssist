{
  "name": "insight",
  "displayName": "Insight",
  "description": "Know your app better",
  "publisher": "raathigeshan",
  "version": "0.5.0",
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Raathigesh/insight.git"
  },
  "icon": "icons/icon.png",
  "galleryBanner": {
    "color": "#111111",
    "theme": "dark"
  },
  "main": "./out/extension/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "insight.showPanel",
        "title": "Toggle Insight panel"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run build",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./src/extension --project ./tsconfig.json",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test",
    "ui": "webpack-dev-server --env.development --config ./webpack.config.js",
    "build-ui": "webpack --env.production --config ./webpack.config.js",
    "build": "rimraf out && yarn compile && yarn build-ui",
    "package-ext": "vsce package",
    "publish-ext": "vsce publish --yarn"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/babel-traverse": "^6.25.5",
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "@types/react-select": "^2.0.17",
    "@types/rebass": "^3.0.3",
    "@types/styled-components": "4.1.10",
    "@types/styled-system": "^4.1.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "graphql-tag": "^2.10.1",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "react": "^16.8.6",
    "react-archer": "^1.1.0",
    "react-color": "^2.17.2",
    "react-dom": "^16.8.6",
    "react-feather": "^1.1.6",
    "react-router-dom": "^5.0.0",
    "react-select": "^3.0.4",
    "react-tippy": "^1.2.3",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "rebass": "^3.1.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "styled-components": "^4.2.0",
    "styled-system": "^4.1.0",
    "subscriptions-transport-ws": "^0.9.16",
    "typescript": "3.3.4000",
    "urql": "^1.0.5",
    "vscode": "^1.1.28",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "@babel/traverse": "^7.4.3",
    "graphql": "^14.3.1",
    "graphql-yoga": "^1.17.4",
    "module-alias": "^2.2.0",
    "recursive-readdir": "^2.2.2",
    "reflect-metadata": "^0.1.13",
    "tmp": "^0.1.0",
    "type-graphql": "^0.17.4",
    "typedi": "^0.8.0",
    "wonka": "^3.2.0",
    "ws": "^7.1.1"
  },
  "resolutions": {
    "graphql": "^14.3.1"
  },
  "_moduleAliases": {
    "common": "./out/common"
  }
}
